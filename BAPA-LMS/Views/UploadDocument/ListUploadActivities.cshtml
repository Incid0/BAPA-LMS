@model IEnumerable<BAPA_LMS.Models.ActivityViewModels.ActivitySubmitViewModel>

@{
    ViewBag.Title = "ListUploadActivities";
}

<h2>Lista över inlämningsaktiviteter</h2>


<table class="table table-striped table-responsive">
    <tr class="custom-table" >
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr class="custom-table" >
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
  
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndTime)
        </td>
        <td>
            @Html.ActionLink("Inlämningsuppgifter","ListDocuments", new { id = item.Id })
         </td>
    </tr>
}

</table>
<hr />
<div>
    @Html.ActionLink("Tillbaka", "Index", "Teacher")
</div>